<?xml version="1.0" encoding="utf-8" ?>
<widget id="FusionChartsPlusWidget.FusionChartsWidget" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
	<name>FusionChartsPlusWidget</name>
	<description>View the Fusionchart chart</description>

	<icon></icon>
	<properties>
		<property key="width" type="integer" defaultValue="500">
			<caption>Width(in pixels)</caption>
			<category>Size</category>
			<description></description>
		</property>
		<property key="height" type="integer" defaultValue="400">
			<caption>Height(in pixels)</caption>
			<category>Size</category>
			<description></description>
		</property>
		<property key="refresh" type="boolean" defaultValue="false">
			<caption>Refresh</caption>
			<category>Widget</category>
			<description>Refresh the Chart</description>
		</property>
		<property key="refreshInterval" type="integer" defaultValue="5">
			<caption>Refresh Interval</caption>
			<category>Widget</category>
			<description>Refresh interval in minutes</description>
		</property>
		<property key="refreshMicroflow" type="microflow" required="false">
			<caption>Refresh Chart</caption>
			<category>Widget</category>
			<description>This microflow will be invoked when the chart automatically refreshes</description>
			<returnType type="Void"/>
		</property>
		<property key="drillDownMicroflow" type="microflow" required="false">
			<caption>Drill Down</caption>
			<category>Widget</category>
			<description>This microflow will be invoked when clicking a dataset of a widget</description>
			<returnType type="Void"/>
		</property>			
		<property key="editNodeMicroflow" type="microflow" required="false">
			<caption>Edit Node Action</caption>
			<category>DragNode chart</category>
			<description>This microflow will be invoked when the user edits a Node.</description>
			<returnType type="Void"/>
		</property>
		<property key="deleteNodeMicroflow" type="microflow" required="false">
			<caption>Delete Node Action</caption>
			<category>DragNode chart</category>
			<description>This microflow will be invoked when the user deletes a Node.</description>
			<returnType type="Void"/>
		</property>
		<property key="addNodeMicroflow" type="microflow" required="false">
			<caption>Add Node Action</caption>
			<category>DragNode chart</category>
			<description>This microflow will be invoked when the user adds a Node.</description>
			<returnType type="Void"/>
		</property>
		<property key="editLinkMicroflow" type="microflow" required="false">
			<caption>Edit Link Action</caption>
			<category>DragNode chart</category>
			<description>This microflow will be invoked when the user edits a Link.</description>
			<returnType type="Void"/>
		</property>
		<property key="deleteLinkMicroflow" type="microflow" required="false">
			<caption>Delete Link Action</caption>
			<category>DragNode chart</category>
			<description>This microflow will be invoked when the user deletes a Link.</description>
			<returnType type="Void"/>
		</property>
		<property key="resetChartMicroflow" type="microflow" required="false">
			<caption>Reset Chart Action</caption>
			<category>DragNode chart</category>
			<description>This microflow will be invoked when the user resets the chart with the selected node.</description>
			<returnType type="Void"/>
		</property>	
	</properties>
</widget>
